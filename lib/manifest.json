{
  "name": "superboost",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "A chrome extension that superboosts your visibility on OkCupid.",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "*://*/",
    "tabs"
  ],
  "background": {
    "scripts": ["index.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "OkCupid"
  }
}
